{
  "do_first_run": "false",
  "btrfs_mode": "false",
  "include_btrfs_home_for_backup": "false",
  "include_btrfs_home_for_restore": "false",
  "stop_cron_emails": "true",
  "schedule_monthly": "true",
  "schedule_weekly": "true",
  "schedule_daily": "true",
  "schedule_hourly": "true",
  "schedule_boot": "false",
  "count_monthly": "2",
  "count_weekly": "4",
  "count_daily": "5",
  "count_hourly": "5",
  "count_boot": "5",
  "date_format": "%Y-%m-%d %H:%M:%S",
  "exclude": [
    ":home:/.local/shared/Trash",
    ":home:/.steam/***",
    "/tmp",
    "***node_modules***",
    "***vendor***",
    "***Cache***",
    "***cache***",
    "***Caches***",
    "***caches***",
    "***dist***",
    "/var/lib/docker",
    "/var/lib/containerd",
    "+ :home:/.**",
    "+ :home:/Code/***",
    "+ :home:/:documents_dir:/***",
    "/root/**"
  ],
  "exclude-apps": []
}
